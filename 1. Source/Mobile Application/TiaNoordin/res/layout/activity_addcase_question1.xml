<?xml version="1.0" encoding="utf-8"?>
<android.support.v4.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent" >

    <!-- The main content view -->

    <FrameLayout
        android:id="@+id/content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <!-- Add content here -->

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"            
            android:orientation="vertical"
             android:background="@drawable/appbg" >

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
              
                android:orientation="vertical" >

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="vertical" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:background="#296894"  >

                        <LinearLayout
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="horizontal" >

                            <TextView
                                android:id="@+id/textView_evoita"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:text="@string/logo"
                                android:textColor="@color/white"
                                android:textSize="40sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView_spa"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/logospa"
                                android:textColor="@color/aqua"
                                android:textSize="20sp"
                                android:textStyle="bold" />

                            <TextView
                                android:id="@+id/textView_welcome"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:paddingLeft="10dp"
                                android:paddingRight="10dp"
                                android:text="@string/welcome"
                                android:textColor="@color/aqua"
                                android:textSize="20sp"
                                android:textStyle="bold" />
                        </LinearLayout>

                        <LinearLayout
                            android:id="@+id/relativeLayout1"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            
                            android:gravity="right" >

                            <Button
                                android:id="@+id/button_AddCaseQuestion1Walkin"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:text="Walk-in"
                                android:textColor="@color/royalBlue" />

                            <Button
                                android:id="@+id/button_AddCaseQuestion1Find"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:clickable="false"
                                android:text="Find"
                                android:textColor="@color/gray" />

                            <Button
                                android:id="@+id/button_AddCaseQuestion1Add"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:clickable="false"
                                android:text="Add"
                                android:textColor="@color/gray" />

                            <Button
                                android:id="@+id/button_AddCaseQuestion1Edit"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:clickable="false"
                                android:text="Edit"
                                android:textColor="@color/gray" />

                            <Button
                                android:id="@+id/button_AddCaseQuestion1Confirm"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:clickable="false"
                                android:text="confirm"
                                android:textColor="@color/gray" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/lineA"
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:background="@color/blueLight"
                        android:orientation="vertical" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:padding="10dp" >

                    <TextView
                        android:id="@+id/textView_question1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="left"
                        android:text="QUESTIONAIRE"
                        android:textColor="@color/whiteSmoke"
                        android:textSize="@dimen/activity_medium_textsize"
                        android:textStyle="bold" />

                    <LinearLayout
                        android:id="@+id/lineC"
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:background="@color/blueLight"
                        android:orientation="vertical" />
                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/paddingLeft_twenty"
                        android:paddingRight="@dimen/paddingRight_twenty" >

                        <TextView
                            android:id="@+id/qtn1"
                            android:layout_width="356dp"
                            android:layout_height="wrap_content"
                            android:text="Are you Buying or Selling ?"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#fff"
                            android:textSize="20sp" />
<RadioGroup 
						android:id="@+id/RadioGroup01" 
						android:layout_height="wrap_content" 
						android:layout_width="fill_parent"
						android:orientation="horizontal"
						android:layout_column="0"
						android:layout_weight="3"
						android:layout_span="3">
                        <RadioButton
                            android:id="@+id/btnsell"
                            android:layout_width="300dp"
                            android:layout_height="wrap_content"
                            android:button="@drawable/myredio"
                            android:textColor="#fff"
                            android:textSize="20sp"
                            android:paddingLeft="5dp"
                            android:text="SELL" />
                         <RadioButton
                            android:id="@+id/btnbuy"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                             android:textColor="#fff"
                             android:textSize="20sp"
                             android:paddingLeft="5dp"
                             android:button="@drawable/myredio"
                            android:text="BUY" />
                        
                        </RadioGroup>
                    </LinearLayout>
                    
                </LinearLayout>
                <LinearLayout
                        android:id="@+id/lineB"
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:background="@color/blueLight"
                        android:orientation="vertical"
                        android:layout_marginTop="30dp"
                         />
                
                
                
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/paddingLeft_twenty"
                        android:paddingRight="@dimen/paddingRight_twenty"
                        android:layout_marginTop="10dp" >

                        <TextView
                            android:id="@+id/qtn2"
                            android:layout_width="356dp"
                            android:layout_height="wrap_content"
                            android:text="Who are you buying from?"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#fff"
                            android:textSize="20sp" />
                        
                          <RadioGroup 
						android:id="@+id/RadioGroup02" 
						android:layout_height="wrap_content" 
						android:layout_width="fill_parent"
						android:orientation="horizontal"
						android:layout_column="0"
						android:layout_weight="3"
						android:layout_span="3">
                       
		                         <RadioButton
				                    android:id="@+id/btnsubsale"
				                    android:layout_width="300dp"
				                    android:layout_height="wrap_content"
				                    android:button="@drawable/myredio"
				                    android:text="SUB-SALE"
				                    android:paddingLeft="5dp"
				                    android:textColor="#fff"
				                    android:textSize="20sp" />

		                         <RadioButton
		                             android:id="@+id/btndeveloper"
		                             android:layout_width="250dp"
		                             android:layout_height="wrap_content"
		                             android:button="@drawable/myredio"
		                             android:text="DEVELOPER"
		                             android:paddingLeft="5dp"
		                             android:textColor="#fff"
		                             android:textSize="20sp" />
				                
				               
				                </RadioGroup>
                        
                    </LinearLayout>
                    
                </LinearLayout>
               

               
				<LinearLayout
                     android:id="@+id/lineD"
                     android:layout_width="fill_parent"
                     android:layout_height="1dp"
                     android:background="@color/blueLight"
                     android:orientation="vertical"
                     android:layout_marginTop="30dp"
                      />
				
				 <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/paddingLeft_twenty"
                        android:paddingRight="@dimen/paddingRight_twenty"
                        android:layout_marginTop="10dp" >

                        <TextView
                            android:id="@+id/qtn3"
                            android:layout_width="356dp"
                            android:layout_height="wrap_content"
                            android:text="Act for Developer in this Purchase?"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#fff"
                            android:textSize="20sp" />
                        
                          <RadioGroup 
						android:id="@+id/RadioGroup03" 
						android:layout_height="wrap_content" 
						android:layout_width="fill_parent"
						android:orientation="horizontal"
						android:layout_column="0"
						android:layout_weight="3"
						android:layout_span="3">
                        <RadioButton
		                            android:id="@+id/btnactyes"
		                            android:layout_width="300dp"
		                            android:layout_height="wrap_content"
		                            android:button="@drawable/myredio"
		                             android:textColor="#fff"
		                             android:textSize="20sp"
		                             android:paddingLeft="5dp"
		                            android:text="YES" />
		                         <RadioButton
				                    android:id="@+id/btnactno"
				                    android:layout_width="250dp"
				                    android:layout_height="wrap_content"
				                    android:button="@drawable/myredio"
				                    android:text="NO"
				                    android:textColor="#fff"
				                    android:paddingLeft="5dp"
				                    android:textSize="20sp" />
				
				                <RadioButton
				                    android:id="@+id/btnactapp"
				                    android:layout_width="wrap_content"
				                    android:layout_height="wrap_content"
				                    android:layout_weight="1"
				                    android:paddingLeft="5dp"
				                    android:button="@drawable/myredio"
				                    android:text="NOT APPLICABLE"
				                    android:textColor="#fff"
				                    android:textSize="20sp" />
				                </RadioGroup>
                        
                    </LinearLayout>
                    
                </LinearLayout>
               

               
 <LinearLayout
                        android:id="@+id/lineE"
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:background="@color/blueLight"
                        android:orientation="vertical"
                        android:layout_marginTop="30dp"
                         />
            </LinearLayout>
            
            
            <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/paddingLeft_twenty"
                        android:paddingRight="@dimen/paddingRight_twenty"
                        android:layout_marginTop="10dp" >

                        <TextView
                            android:id="@+id/qtn4"
                            android:layout_width="356dp"
                            android:layout_height="wrap_content"
                            android:text="What is the type of Property?"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#fff"
                            android:textSize="20sp" />
                        
                         <RadioGroup 
						android:id="@+id/RadioGroup04" 
						android:layout_height="wrap_content" 
						android:layout_width="fill_parent"
						android:orientation="horizontal"
						android:layout_column="0"
						android:layout_weight="3"
						android:layout_span="3">
                        <RadioButton
		                            android:id="@+id/btncomm"
		                            android:layout_width="300dp"
		                            android:layout_height="wrap_content"
		                            android:button="@drawable/myredio"
		                             android:textColor="#fff"
		                             android:textSize="20sp"
		                             android:paddingLeft="5dp"
		                            android:text="COMMERCIAL" />
		                         <RadioButton
				                    android:id="@+id/btnlan"
				                    android:layout_width="250dp"
				                    android:layout_height="wrap_content"
				                    android:button="@drawable/myredio"
				                    android:text="LANDED"
				                    android:paddingLeft="5dp"
				                    android:textColor="#fff"
				                    android:textSize="20sp" />
				
				                <RadioButton
				                    android:id="@+id/btnaprt"
				                    android:layout_width="wrap_content"
				                    android:layout_height="wrap_content"
				                    android:layout_weight="1"
				                    android:paddingLeft="5dp"
				                    android:text="APARTMENT / CONDO"
				                    android:textColor="#fff"
				                    android:button="@drawable/myredio"
				                    android:textSize="20sp" />
				                </RadioGroup>
                        
                    </LinearLayout>
                    
                </LinearLayout>
               

             
 <LinearLayout
                        android:id="@+id/lineF"
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:background="@color/blueLight"
                        android:orientation="vertical"
                        android:layout_marginTop="30dp"
                         />
                    
 
 
             <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp" >

                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/paddingLeft_twenty"
                        android:paddingRight="@dimen/paddingRight_twenty"
                        android:layout_marginTop="10dp" >

                        <TextView
                            android:id="@+id/qtn5"
                            android:layout_width="356dp"
                            android:layout_height="wrap_content"
                            android:text="Is the Property Under Construction?"
                            android:textAppearance="?android:attr/textAppearanceMedium"
                            android:textColor="#fff"
                            android:textSize="20sp" />
                        
                           <RadioGroup 
						android:id="@+id/RadioGroup05" 
						android:layout_height="wrap_content" 
						android:layout_width="fill_parent"
						android:orientation="horizontal"
						android:layout_column="0"
						android:layout_weight="3"
						android:layout_span="3">

                        <RadioButton
                            android:id="@+id/btnuc"
                            android:layout_width="300dp"
                            android:layout_height="wrap_content"
                            android:text="UNDER CONSTRUCTION"
                            android:textColor="#fff"
                            android:button="@drawable/myredio"
                            android:paddingLeft="5dp"
                            android:textSize="20sp" />

		                         <RadioButton
				                    android:id="@+id/btncomp"
				                    android:layout_width="wrap_content"
				                    android:layout_height="wrap_content"
				                    android:button="@drawable/myredio"
				                    android:text="COMPLETED"
				                    android:paddingLeft="5dp"
				                    android:textColor="#fff"
				                    android:textSize="20sp" />
				                </RadioGroup>
                        
                    </LinearLayout>
                    
                </LinearLayout>
               

               
 <LinearLayout
                        android:id="@+id/lineG"
                        android:layout_width="fill_parent"
                        android:layout_height="1dp"
                        android:background="@color/blueLight"
                        android:orientation="vertical"
                        android:layout_marginTop="30dp"
                         />

  <Button
                    android:id="@+id/btn_confirm"
                    android:layout_width="200dp"
                    android:layout_height="48dp"
                    android:layout_gravity="center"
                    android:background="@drawable/loginbtn"
                    android:gravity="center_vertical|center_horizontal"
                    android:text="Confirm"
                    android:textColor="@color/white"
                     android:layout_marginTop="30dp" />
            
        </LinearLayout>
  
        </FrameLayout>

     <!-- The navigation drawer -->

    <ListView
        android:id="@+id/left_drawer"
        android:layout_width="260dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:background="#ffffff"
        android:choiceMode="singleChoice"
        android:divider="#5b5b5b"
        android:dividerHeight="1dp"
        android:paddingBottom="@dimen/paddingBottom_twenty"        
        android:paddingTop="@dimen/paddingTop_twenty" />

</android.support.v4.widget.DrawerLayout>